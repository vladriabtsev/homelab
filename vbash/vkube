vkube_cluster_install_usage() {
  printf "vkube cluster install - Install cluster kubernetes, storage drivers, applications\n\n"

  printf "%s\n" "$(bold "Usage:")"
  printf "  vkube k3s install [OPTIONS]\n"
  printf "  vkube k3s install --help | -h\n"
  echo

  # :command.long_usage
  if [[ -n "$long_usage" ]]; then
    printf "%s\n" "$(bold "Options:")"

    # :command.usage_flags
    # :flag.usage
    printf "  %s\n" "$(magenta "--core")"
    printf "    Core cluster components.\n"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--storage-classes-only")"
    printf "    Only storage classes for general storage drivers.\n"
    printf "    %s\n" "Conflicts: --storage --csi-driver-nfs --csi-driver-smb --csi-synology --longhorn"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--storage")"
    printf "    All general storage drivers according cluster plan.\n"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--velero")"
    printf "    Velero is an open source tool to safely backup and restore, perform disaster\n    recovery, and migrate Kubernetes cluster resources and persistent volumes.\n"
    printf "    %s\n" "Conflicts: --storage"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--csi-driver-nfs")"
    printf "    CSI driver NFS storage. Overriding storage use in cluster plan. Install all\n    if --core, --storage, and --app flags are not set.\n"
    printf "    %s\n" "Conflicts: --storage"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--csi-driver-smb")"
    printf "    CSI driver SMB storage. Overriding storage use in cluster plan. Install all\n    if --core, --storage, and --app flags are not set.\n"
    printf "    %s\n" "Conflicts: --storage"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--csi-synology")"
    printf "    Synology CSI storage (NFS, SMB, ISCSI). Overriding storage use in cluster\n    plan. Install all if --core, --storage, and --app flags are not set.\n"
    printf "    %s\n" "Conflicts: --storage"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--csi-driver-direct-pv")"
    printf "    MinIO Direct PV storage. Overriding storage use in cluster plan. Install all\n    if --core, --storage, and --app flags are not set.\n"
    printf "    %s\n" "Conflicts: --storage"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--longhorn")"
    printf "    Longhorn storage. Overriding storage use in cluster plan. Install all if\n    --core, --storage, and --app flags are not set.\n"
    printf "    %s\n" "Conflicts: --storage"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--apps")"
    printf "    All applications from cluster plan.\n"
    printf "    %s\n" "Conflicts: --app"
    echo

    # :flag.usage
    printf "  %s\n" "$(magenta "--app APP-NAME (repeatable)")"
    printf "    Only specified applications by name from cluster plan.\n"
    printf "    %s\n" "Conflicts: --apps"
    echo

    # :command.usage_fixed_flags
    printf "  %s\n" "$(magenta "--help, -h")"
    printf "    Show this help\n"
    echo

  fi
}


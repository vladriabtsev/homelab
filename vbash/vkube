# :command.parse_requirements
vkube_app_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        vkube_app_usage
        exit
        ;;

      # :flag.case
      --namespace | -n)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--namespace']="$2"
          shift
          shift
        else
          printf "%s\n" "--namespace requires an argument: --namespace, -n NAMESPACE" >&2
          exit 1
        fi
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action=${1:-}

  case $action in
    -*) ;;

    install)
      action="install"
      shift
      vkube_app_install_parse_requirements "$@"
      shift $#
      ;;

    uninstall)
      action="uninstall"
      shift
      vkube_app_uninstall_parse_requirements "$@"
      shift $#
      ;;

    # :command.command_fallback
    "")
      vkube_app_usage >&2
      exit 1
      ;;

    *)
      printf "invalid command: %s\n" "$action" >&2
      exit 1
      ;;

  esac

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        printf "invalid argument: %s\n" "$key" >&2
        exit 1

        ;;

    esac
  done

  # :command.default_assignments
  [[ -n ${args['--namespace']:-} ]] || args['--namespace']="default"

}

# :command.parse_requirements
vkube_app_install_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        vkube_app_install_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action=${1:-}

  case $action in
    -*) ;;

    general)
      action="general"
      shift
      vkube_app_install_general_parse_requirements "$@"
      shift $#
      ;;

    busybox)
      action="busybox"
      shift
      vkube_app_install_busybox_parse_requirements "$@"
      shift $#
      ;;

    # :command.command_fallback
    "")
      vkube_app_install_usage >&2
      exit 1
      ;;

    *)
      printf "invalid command: %s\n" "$action" >&2
      exit 1
      ;;

  esac

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        printf "invalid argument: %s\n" "$key" >&2
        exit 1

        ;;

    esac
  done

}

# :command.parse_requirements
vkube_app_install_general_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        vkube_app_install_general_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="app install general"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      # :flag.case
      --storage-class | -c)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          escaped="$(printf '%q' "$2")"
          if [[ -z ${args['--storage-class']+x} ]]; then
            args['--storage-class']="$escaped"
          elif [[ -z "${unique_lookup["--storage-class:${escaped}"]:-}" ]]; then
            args['--storage-class']="${args['--storage-class']} $escaped"
          fi
          unique_lookup["--storage-class:${escaped}"]=1
          shift
          shift
        else
          printf "%s\n" "--storage-class requires an argument: --storage-class, -c NAME" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --mount-point | -m)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          escaped="$(printf '%q' "$2")"
          if [[ -z ${args['--mount-point']+x} ]]; then
            args['--mount-point']="$escaped"
          elif [[ -z "${unique_lookup["--mount-point:${escaped}"]:-}" ]]; then
            args['--mount-point']="${args['--mount-point']} $escaped"
          fi
          unique_lookup["--mount-point:${escaped}"]=1
          shift
          shift
        else
          printf "%s\n" "--mount-point requires an argument: --mount-point, -m NAME" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --all-storage-classes)

        # :flag.case_no_arg
        args['--all-storage-classes']=1
        shift
        ;;

      # :flag.case
      --all-storage-classes-ext)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--all-storage-classes-ext']="$2"
          shift
          shift
        else
          printf "%s\n" "--all-storage-classes-ext requires an argument: --all-storage-classes-ext STORAGE-CLASSES-EXT" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --deployment | -d)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--deployment']="$2"
          shift
          shift
        else
          printf "%s\n" "--deployment requires an argument: --deployment, -d DEPLOYMENT" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --release | -r)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--release']="$2"
          shift
          shift
        else
          printf "%s\n" "--release requires an argument: --release, -r RELEASE" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --storage-size | -s)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--storage-size']="$2"
          shift
          shift
        else
          printf "%s\n" "--storage-size requires an argument: --storage-size, -s STORAGE-SIZE-REQUEST" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --access-mode | -a)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--access-mode']="$2"
          shift
          shift
        else
          printf "%s\n" "--access-mode requires an argument: --access-mode, -a ACCESS-MODE" >&2
          exit 1
        fi
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        # :argument.case
        if [[ -z ${args['container-name']+x} ]]; then
          args['container-name']=$1
          shift
        else
          printf "invalid argument: %s\n" "$key" >&2
          exit 1
        fi

        ;;

    esac
  done

  # :command.required_args_filter
  if [[ -z ${args['container-name']+x} ]]; then
    printf "missing required argument: CONTAINER-NAME\nusage: vkube app install general CONTAINER-NAME [OPTIONS]\n" >&2

    exit 1
  fi

  # :command.default_assignments
  [[ -n ${args['--all-storage-classes-ext']:-} ]] || args['--all-storage-classes-ext']="del"
  [[ -n ${args['--release']:-} ]] || args['--release']="latest"
  [[ -n ${args['--storage-size']:-} ]] || args['--storage-size']="1Gi"
  [[ -n ${args['--access-mode']:-} ]] || args['--access-mode']="ReadWriteOnce"

  # :command.whitelist_filter
  if [[ ${args['--all-storage-classes-ext']:-} ]] && [[ ! ${args['--all-storage-classes-ext']:-} =~ ^(del|ret|ext)$ ]]; then
    printf "%s\n" "--all-storage-classes-ext must be one of: del, ret, ext" >&2
    exit 1
  fi
  if [[ ${args['--access-mode']:-} ]] && [[ ! ${args['--access-mode']:-} =~ ^(ReadWriteOnce|ReadOnlyMany|ReadWriteMany|ReadWriteOncePod)$ ]]; then
    printf "%s\n" "--access-mode must be one of: ReadWriteOnce, ReadOnlyMany, ReadWriteMany, ReadWriteOncePod" >&2
    exit 1
  fi

}

# :command.parse_requirements
vkube_app_install_busybox_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        vkube_app_install_busybox_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="app install busybox"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      # :flag.case
      --variant)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--variant']="$2"
          shift
          shift
        else
          printf "%s\n" "--variant requires an argument: --variant VARIANT" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --storage-class | -c)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          escaped="$(printf '%q' "$2")"
          if [[ -z ${args['--storage-class']+x} ]]; then
            args['--storage-class']="$escaped"
          elif [[ -z "${unique_lookup["--storage-class:${escaped}"]:-}" ]]; then
            args['--storage-class']="${args['--storage-class']} $escaped"
          fi
          unique_lookup["--storage-class:${escaped}"]=1
          shift
          shift
        else
          printf "%s\n" "--storage-class requires an argument: --storage-class, -c NAME" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --mount-point | -m)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          escaped="$(printf '%q' "$2")"
          if [[ -z ${args['--mount-point']+x} ]]; then
            args['--mount-point']="$escaped"
          elif [[ -z "${unique_lookup["--mount-point:${escaped}"]:-}" ]]; then
            args['--mount-point']="${args['--mount-point']} $escaped"
          fi
          unique_lookup["--mount-point:${escaped}"]=1
          shift
          shift
        else
          printf "%s\n" "--mount-point requires an argument: --mount-point, -m NAME" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --all-storage-classes)

        # :flag.case_no_arg
        args['--all-storage-classes']=1
        shift
        ;;

      # :flag.case
      --all-storage-classes-ext)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--all-storage-classes-ext']="$2"
          shift
          shift
        else
          printf "%s\n" "--all-storage-classes-ext requires an argument: --all-storage-classes-ext STORAGE-CLASSES-EXT" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --deployment | -d)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--deployment']="$2"
          shift
          shift
        else
          printf "%s\n" "--deployment requires an argument: --deployment, -d DEPLOYMENT" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --release | -r)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--release']="$2"
          shift
          shift
        else
          printf "%s\n" "--release requires an argument: --release, -r RELEASE" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --storage-size | -s)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--storage-size']="$2"
          shift
          shift
        else
          printf "%s\n" "--storage-size requires an argument: --storage-size, -s STORAGE-SIZE-REQUEST" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --access-mode | -a)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--access-mode']="$2"
          shift
          shift
        else
          printf "%s\n" "--access-mode requires an argument: --access-mode, -a ACCESS-MODE" >&2
          exit 1
        fi
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        printf "invalid argument: %s\n" "$key" >&2
        exit 1

        ;;

    esac
  done

  # :command.default_assignments
  [[ -n ${args['--variant']:-} ]] || args['--variant']="musl"
  [[ -n ${args['--all-storage-classes-ext']:-} ]] || args['--all-storage-classes-ext']="del"
  [[ -n ${args['--release']:-} ]] || args['--release']="latest"
  [[ -n ${args['--storage-size']:-} ]] || args['--storage-size']="1Gi"
  [[ -n ${args['--access-mode']:-} ]] || args['--access-mode']="ReadWriteOnce"

  # :command.whitelist_filter
  if [[ ${args['--variant']:-} ]] && [[ ! ${args['--variant']:-} =~ ^(musl|uclibc|glibc)$ ]]; then
    printf "%s\n" "--variant must be one of: musl, uclibc, glibc" >&2
    exit 1
  fi
  if [[ ${args['--all-storage-classes-ext']:-} ]] && [[ ! ${args['--all-storage-classes-ext']:-} =~ ^(del|ret|ext)$ ]]; then
    printf "%s\n" "--all-storage-classes-ext must be one of: del, ret, ext" >&2
    exit 1
  fi
  if [[ ${args['--access-mode']:-} ]] && [[ ! ${args['--access-mode']:-} =~ ^(ReadWriteOnce|ReadOnlyMany|ReadWriteMany|ReadWriteOncePod)$ ]]; then
    printf "%s\n" "--access-mode must be one of: ReadWriteOnce, ReadOnlyMany, ReadWriteMany, ReadWriteOncePod" >&2
    exit 1
  fi

}

# :command.parse_requirements
vkube_app_uninstall_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        vkube_app_uninstall_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action=${1:-}

  case $action in
    -*) ;;

    deployment)
      action="deployment"
      shift
      vkube_app_uninstall_deployment_parse_requirements "$@"
      shift $#
      ;;

    # :command.command_fallback
    "")
      vkube_app_uninstall_usage >&2
      exit 1
      ;;

    *)
      printf "invalid command: %s\n" "$action" >&2
      exit 1
      ;;

  esac

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        printf "invalid argument: %s\n" "$key" >&2
        exit 1

        ;;

    esac
  done

}

# :command.parse_requirements
vkube_app_uninstall_deployment_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        vkube_app_uninstall_deployment_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="app uninstall deployment"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        # :argument.case
        if [[ -z ${args['general-deployment-name']+x} ]]; then
          args['general-deployment-name']=$1
          shift
        else
          printf "invalid argument: %s\n" "$key" >&2
          exit 1
        fi

        ;;

    esac
  done

  # :command.required_args_filter
  if [[ -z ${args['general-deployment-name']+x} ]]; then
    printf "missing required argument: GENERAL-DEPLOYMENT-NAME\nusage: vkube app uninstall deployment GENERAL-DEPLOYMENT-NAME\n" >&2

    exit 1
  fi

}

# :command.user_hooks
before_hook() {
  # src/before.sh
  #!/usr/bin/env bash
  #inspect_args

  # shellcheck source=/dev/null
  source "${VBASH}/vlib.bash"

  vkube_folder="$(dirname $0)"
  vlib.trace "vkube_folder=$vkube_folder"

}

# :command.initialize
bashly-initialize() {
  declare -g version="0.1.0"
  set -e

}

# :command.run
bashly-run() {
  # :command.globals
  declare -g long_usage=''
  declare -g -A args=()
  declare -g -A deps=()
  declare -g -a env_var_names=()
  declare -g -a input=()
  declare -g -A unique_lookup=()

  normalize_input "$@"
  parse_requirements "${input[@]}"
  before_hook

  case "$action" in
    "exec") vkube_exec_command ;;
    "cluster") vkube_cluster_command ;;
    "cluster install") vkube_cluster_install_command ;;
    "cluster delete") vkube_cluster_delete_command ;;
    "cluster get") vkube_cluster_get_command ;;
    "cluster get storage-classes") vkube_cluster_get_storage_classes_command ;;
    "cluster get containers") vkube_cluster_get_containers_command ;;
    "cluster storage-speed") vkube_cluster_storage_speed_command ;;
    "cluster upgrade") vkube_cluster_upgrade_command ;;
    "cluster backup") vkube_cluster_backup_command ;;
    "cluster restore") vkube_cluster_restore_command ;;
    "app") vkube_app_command ;;
    "app install") vkube_app_install_command ;;
    "app install general") vkube_app_install_general_command ;;
    "app install busybox") vkube_app_install_busybox_command ;;
    "app uninstall") vkube_app_uninstall_command ;;
    "app uninstall deployment") vkube_app_uninstall_deployment_command ;;
  esac
}

if [[ "${BASH_SOURCE[0]}" == "${0}" ]]; then
  bashly-initialize
  bashly-run "$@"
fi
